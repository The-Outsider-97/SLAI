{
  "hyperparameters": [
    {
      "name": "learning_rate",
      "type": "float",
      "min": 0.0001,
      "max": 0.1,
      "prior": "log-uniform",
      "steps": 5
    },
    {
      "name": "num_layers",
      "type": "int",
      "min": 1,
      "max": 10,
      "step": 1
    },
    {
      "name": "batch_size",
      "type": "int",
      "min": 16,
      "max": 256,
      "step": 16
    },
    {
      "name": "optimizer",
      "type": "categorical",
      "choices": ["adam", "sgd", "rmsprop"]
    },
    {
      "name": "activation",
      "type": "categorical",
      "choices": ["relu", "tanh", "sigmoid"]
    },
    {
      "name": "dropout_rate",
      "type": "float",
      "min": 0.0,
      "max": 0.5,
      "steps": 6
    },
    {
      "name": "gamma",
      "type": "float",
      "min": 0.8,
      "max": 0.999,
      "steps": 5
    }
  ]
}
